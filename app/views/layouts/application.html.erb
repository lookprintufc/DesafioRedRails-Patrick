               <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><div class="avatar_menu"><img alt="Patrick" height="28" src="<%=@user.profile_pic || '' %>" width="28" class="image_1pa4v1p"></div> <span class="caret"></span></a>
