      </div>

       <!-- /container --> 
      <% if @user.nil? || @user.id.nil? %>
      <%= render "layouts/modal/signup" %>
      <%= render "layouts/modal/signin" %>
      <% end %>
      <%= javascript_include_tag 'application' %>



      <% flash.each do |key, value| %>
      <script type="text/javascript">
         toastr.warning("<span><%= value %></span>", "Aviso");
      </script>
      <% end %>




   </body>
</html>