               <% unless @user.nil? || @user.id.nil? %>
               <li><%= link_to 'Torne-se um anfitrião', places_path %></li>
               <% end %>
               <li><a href="#">Ajuda</a></li>
               <% if @user.nil? || @user.id.nil? %>
               <li><a class="modal-open" href="#login" data-toggle="modal">Cadastre-se</a></li>
               <li><a class="modal-open" href="#login-in" data-toggle="modal">Entrar</a></li>
